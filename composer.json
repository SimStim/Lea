{
  "name": "simstim/lea",
  "description": "Lea: ePub anvil",
  "type": "project",
  "license": "PolyForm-Noncommercial-1.0.0",
  "require": {
    "php": "^8.5",
    "ext-mbstring": "*",
    "ext-libxml": "*",
    "ext-dom": "*"
  },
  "require-dev": {
    "sebastian/version": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "Lea\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lea\\Tests\\": "tests/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  }
}